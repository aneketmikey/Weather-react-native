{
  "expo": {
    "name": "Weather APP",
    "description": "This project is really great.",
    "slug": "my-weather-app",
    "privacy": "unlisted",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
},
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "my.aneketwork.exponative.myweatherapp",
      "supportsTablet": true
    },
    "android":{
      "package":"my.aneketwork.exponative.myweatherapp",
      "icon":"./assets/icon.png",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
      "ACCESS_FINE_LOCATION"
      ]


    }
  }
}
